{
  "name": "gatsby-with-tailwind",
  "version": "1.0.0",
  "private": true,
  "description": "Gatsby With Tailwind",
  "author": "Matheus Rocha",
  "license": "BSD-3-Clause",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "dev": "npm run develop",
    "start": "gatsby develop",
    "build": "gatsby build --log-pages",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write src/**/*.{js,jsx}",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*": [
      "npm run format"
    ]
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "gatsby": "^3.6.2",
    "gatsby-image": "^3.6.0",
    "gatsby-plugin-image": "^1.6.0",
    "gatsby-plugin-manifest": "^3.6.0",
    "gatsby-plugin-mdx": "^2.6.0",
    "gatsby-plugin-netlify-cms": "^5.6.0",
    "gatsby-plugin-postcss": "^4.6.0",
    "gatsby-plugin-react-helmet": "^4.6.0",
    "gatsby-plugin-sharp": "^3.6.0",
    "gatsby-plugin-sitemap": "^4.2.0",
    "gatsby-remark-autolink-headers": "^4.3.0",
    "gatsby-remark-images": "^5.3.0",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-transformer-sharp": "^3.6.0",
    "netlify-cms-app": "^2.15.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "rehype-accessible-emojis": "^0.3.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.2.6",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.0",
    "tailwindcss": "^2.1.4"
  }
}
